{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/mcp.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/oauth-discovery.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/oauth-authorize.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/oauth-token.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/oauth-revoke.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/oauth-associate.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/mcp",
      "dest": "/api/mcp.ts"
    },
    {
      "src": "/mcp",
      "dest": "/api/mcp.ts"
    },
    {
      "src": "/api/oauth/.well-known/oauth-authorization-server",
      "dest": "/api/oauth-discovery.ts"
    },
    {
      "src": "/api/oauth/authorize",
      "dest": "/api/oauth-authorize.ts"
    },
    {
      "src": "/api/oauth/token",
      "dest": "/api/oauth-token.ts"
    },
    {
      "src": "/api/oauth/revoke",
      "dest": "/api/oauth-revoke.ts"
    },
    {
      "src": "/api/oauth-associate",
      "dest": "/api/oauth-associate.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
