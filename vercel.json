{
  "version": 2,
  "name": "cod-loadout-pro",
  "builds": [
    {
      "src": "api/mcp.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/mcp",
      "dest": "/api/mcp.ts",
      "methods": ["POST", "GET", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/mcp.ts"
    }
  ],
  "functions": {
    "api/mcp.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}